<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://mbws/Configuration">
	<accountserver>
		<basicAttributes>
			<c2sport>5000</c2sport>
			<queueworkersize>1</queueworkersize>
			<startup>true</startup>
		</basicAttributes>
		<s2sport>5001</s2sport>
		<eventControllers>
			<eventController type="0" class="de.mbws.server.account.controller.LoginEventController"/>
			<eventController type="1" class="de.mbws.server.account.controller.AccountEventController"/>
			<eventController type="2" class="de.mbws.server.account.controller.CharacterEventController"/>
		</eventControllers>
	</accountserver>
	<worldserver>
		<basicAttributes>
			<c2sport>5001</c2sport>
			<queueworkersize>1</queueworkersize>
			<startup>true</startup>
		</basicAttributes>
		<eventControllers>
			<eventController type="0" class="de.mbws.server.world.controller.LoginEventController"/>
			<eventController type="2" class="de.mbws.server.world.controller.CharacterEventController"/>
 			<eventController type="10" class="de.mbws.server.world.controller.MovementEventController"/>
		</eventControllers>
		<accountserverip>localhost</accountserverip>
		<accountserverport>5000</accountserverport>
	</worldserver>
</configuration>