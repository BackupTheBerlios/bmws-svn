	</gameserver>
	<zoneserver>
		<name>Zone 1</name>
		<gameserverip>localhost</gameserverip>
		<gameserverport>5002</gameserverport>
		<mapid>1</mapid>
		<port>5020</port>
		<startup>true</startup>
	</zoneserver>
