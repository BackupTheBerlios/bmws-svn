<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://mbws/Configuration">
	<accountserver>
		<basicAttributes>
			<c2sport>5000</c2sport>
			<queueworkersize>1</queueworkersize>
			<startup>true</startup>
		</basicAttributes>
		<s2sport>5001</s2sport>
		<eventControllers>
			<eventController type="0" class="de.mbws.server.account.controller.LoginEventController"/>
			<eventController type="1" class="de.mbws.server.account.controller.AccountEventController"/>
			<eventController type="2" class="de.mbws.server.account.controller.CharacterEventController"/>
			<eventController type="10" class="de.mbws.server.controller.MovementEventController"/>
		</eventControllers>
	</accountserver>
	<gameserver>
		<port>5002</port>
		<accountserverip>localhost</accountserverip>
		<accountserverport>5002</accountserverport>		
