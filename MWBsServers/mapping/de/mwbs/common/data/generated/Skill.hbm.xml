<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mwbs.common.data.generated.Skill" 
    table="skill"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="skillGroupId"
        type="int"
        column="skill_group_id"
        not-null="true"
        length="10"
    />
    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        length="45"
    />
    <property
        name="availableatstartup"
        type="byte"
        column="availableatstartup"
        not-null="true"
        length="1"
    />
    <property
        name="oppositeskill"
        type="java.lang.Integer"
        column="oppositeskill"
        length="10"
    />
    <property
        name="costhealth"
        type="int"
        column="costhealth"
        not-null="true"
        length="10"
    />
    <property
        name="coststamina"
        type="int"
        column="coststamina"
        not-null="true"
        length="10"
    />
    <property
        name="costmana"
        type="int"
        column="costmana"
        not-null="true"
        length="10"
    />
    <property
        name="xpcostsbuying"
        type="int"
        column="xpcostsbuying"
        not-null="true"
        length="10"
    />
    <property
        name="goldcostsbuying"
        type="int"
        column="goldcostsbuying"
        not-null="true"
        length="10"
    />
    <property
        name="xpcostEnhancement"
        type="int"
        column="xpcost_enhancement"
        not-null="true"
        length="10"
    />
    <property
        name="basevalueFormula"
        type="java.lang.Integer"
        column="basevalue_formula"
        length="10"
    />
    <property
        name="useDuration"
        type="int"
        column="use_duration"
        not-null="true"
        length="10"
    />
    <property
        name="useTime"
        type="int"
        column="use_time"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to CharacterSkillMapping -->
    <set
        name="characterSkillMappings"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="skill_id" />
        </key>
        <one-to-many 
            class="de.mwbs.common.data.generated.CharacterSkillMapping"
        />
    </set>
    <!-- bi-directional one-to-many association to SkillRaceMapping -->
    <set
        name="skillRaceMappings"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="skill_id" />
        </key>
        <one-to-many 
            class="de.mwbs.common.data.generated.SkillRaceMapping"
        />
    </set>
    <!-- bi-directional one-to-many association to NpcSkillMapping -->
    <set
        name="npcSkillMappings"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="skill_id" />
        </key>
        <one-to-many 
            class="de.mwbs.common.data.generated.NpcSkillMapping"
        />
    </set>

</class>
</hibernate-mapping>
