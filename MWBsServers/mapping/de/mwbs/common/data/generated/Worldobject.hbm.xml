<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mwbs.common.data.generated.Worldobject" 
    table="worldobject"
>

    <id
        name="id"
        type="java.lang.Long"
        column="id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        length="255"
    />
    <property
        name="description"
        type="java.lang.String"
        column="description"
        not-null="true"
        length="65535"
    />
    <property
        name="coordinateX"
        type="int"
        column="coordinate_x"
        not-null="true"
        length="10"
    />
    <property
        name="coordinateY"
        type="int"
        column="coordinate_y"
        not-null="true"
        length="10"
    />
    <property
        name="coordinateZ"
        type="int"
        column="coordinate_z"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to WorldobjectType -->
    <many-to-one
        name="worldobjectType"
        class="de.mwbs.common.data.generated.WorldobjectType"
        not-null="true"
    >
        <column name="worldobject_type_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Map -->
    <many-to-one
        name="map"
        class="de.mwbs.common.data.generated.Map"
        not-null="true"
    >
        <column name="map_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to NpcWorldobjectMapping -->
    <set
        name="npcWorldobjectMappings"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="worldobject_id" />
        </key>
        <one-to-many 
            class="de.mwbs.common.data.generated.NpcWorldobjectMapping"
        />
    </set>
    <!-- bi-directional one-to-many association to CharacterWorldobjectMapping -->
    <set
        name="characterWorldobjectMappings"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="worldobject_id" />
        </key>
        <one-to-many 
            class="de.mwbs.common.data.generated.CharacterWorldobjectMapping"
        />
    </set>

</class>
</hibernate-mapping>
