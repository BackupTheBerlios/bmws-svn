<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.generated.CharacterSkillMapping" 
    table="character_skill_mapping"
    lazy="false"
>

    <composite-id name="comp_id" class="de.mbws.common.data.generated.CharacterSkillMappingPK">
        <key-property 
            name="characterdataId" 
            column="characterdata_id" 
            type="java.lang.Long"
            length="20"
        />
        <key-property 
            name="skillId" 
            column="skill_id" 
            type="java.lang.Integer"
            length="10"
        />
    </composite-id>    

    <property
        name="bonusvalue"
        type="int"
        column="bonusvalue"
        not-null="true"
        length="10"
    />
    <property
        name="basevalue"
        type="int"
        column="basevalue"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
    <!-- derived association(s) for compound key -->
    <!-- bi-directional many-to-one association to Skill -->
    <many-to-one
        name="skill"
	    class="de.mbws.common.data.generated.Skill"
	    update="false"
	    insert="false"
	>
    	<column name="skill_id" />
	</many-to-one>
	
    <!-- bi-directional many-to-one association to Characterdata -->
    <many-to-one
        name="characterdata"
	    class="de.mbws.common.data.generated.Characterdata"
	    update="false"
	    insert="false"
	>
    	<column name="characterdata_id" />
	</many-to-one>
	
    <!-- end of derived association(s) -->
  

</class>
</hibernate-mapping>
