<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.generated.Map" 
    table="map"
>

    <id
        name="id"
        type="java.lang.Object"
        column="id"
    >
		    <meta attribute="field-description" inherit="false">
		       auto_increment
		    </meta>
    
        <generator class="assigned" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        length="45"
    />
    <property
        name="filename"
        type="java.lang.String"
        column="filename"
        not-null="true"
        length="255"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to CharacterStatus -->

    <set
        name="characterStatuses"
        lazy="true"
        inverse="true"
		cascade="none"

    >
        <key>
            <column name="map_id" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.CharacterStatus"
        />
    </set>
    <!-- bi-directional one-to-many association to ZoneserverMapMapping -->

    <set
        name="zoneserverMapMappings"
        lazy="true"
        inverse="true"
		cascade="none"

    >
        <key>
            <column name="map_id" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.ZoneserverMapMapping"
        />
    </set>
    <!-- bi-directional one-to-many association to NpcStatus -->

    <set
        name="npcStatuses"
        lazy="true"
        inverse="true"
		cascade="none"

    >
        <key>
            <column name="map_id" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.NpcStatus"
        />
    </set>
    <!-- bi-directional one-to-many association to Worldobject -->

    <set
        name="worldobjects"
        lazy="true"
        inverse="true"
		cascade="none"

    >
        <key>
            <column name="map_id" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.Worldobject"
        />
    </set>

</class>
</hibernate-mapping>
