<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.generated.Language" 
    table="languages"
    lazy="false"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="shortname"
        type="java.lang.String"
        column="shortname"
        not-null="true"
        length="255"
    />
    <property
        name="description"
        type="java.lang.String"
        column="description"
        not-null="true"
        length="255"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to LanguageTextMapping -->

    <set
        name="languageTextMappings"
        lazy="true"
        inverse="true"
		cascade="all"

    >
        <key>
            <column name="languages_id" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.LanguageTextMapping"
        />
    </set>

</class>
</hibernate-mapping>
