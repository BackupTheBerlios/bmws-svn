<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.generated.NpcSkillMapping" 
    table="npc_skill_mapping"
>

    <composite-id name="comp_id" class="de.mbws.common.data.generated.NpcSkillMappingPK">
        <key-property 
            name="npcId" 
            column="npc_id" 
            type="java.lang.Long"
            length="20"
        />
        <key-property 
            name="skillId" 
            column="skill_id" 
            type="java.lang.Object"
            length="10"
        />
    </composite-id>    

    <property
        name="value"
        type="java.lang.Object"
        column="value"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
    <!-- derived association(s) for compound key -->
    <!-- bi-directional many-to-one association to Skill -->
    <many-to-one
        name="skill"
	    class="de.mbws.common.data.generated.Skill"
	    update="false"
	    insert="false"
	>
    	<column name="skill_id" />
	</many-to-one>
	
    <!-- bi-directional many-to-one association to Npc -->
    <many-to-one
        name="npc"
	    class="de.mbws.common.data.generated.Npc"
	    update="false"
	    insert="false"
	>
    	<column name="npc_id" />
	</many-to-one>
	
    <!-- end of derived association(s) -->
  

</class>
</hibernate-mapping>
