<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.generated.Npc" 
    table="npc"
    lazy="false"
>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="NAME"
        not-null="true"
        length="255"
    />
    <property
        name="health"
        type="int"
        column="HEALTH"
        not-null="true"
        length="10"
    />
    <property
        name="mana"
        type="int"
        column="MANA"
        not-null="true"
        length="10"
    />
    <property
        name="stamina"
        type="int"
        column="STAMINA"
        not-null="true"
        length="10"
    />
    <property
        name="age"
        type="int"
        column="AGE"
        not-null="true"
        length="10"
    />
    <property
        name="strength"
        type="int"
        column="STRENGTH"
        not-null="true"
        length="10"
    />
    <property
        name="intelligence"
        type="int"
        column="INTELLIGENCE"
        not-null="true"
        length="10"
    />
    <property
        name="dexterity"
        type="int"
        column="DEXTERITY"
        not-null="true"
        length="10"
    />
    <property
        name="constitution"
        type="int"
        column="CONSTITUTION"
        not-null="true"
        length="10"
    />
    <property
        name="isspecial"
        type="byte"
        column="ISSPECIAL"
        not-null="true"
        length="1"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Race -->
    <many-to-one
        name="race"
        class="de.mbws.common.data.generated.Race"
        not-null="true"
    >
        <column name="RACE_ID" />
    </many-to-one>
    <!-- bi-directional one-to-one association to NpcStatus -->
    <one-to-one
        name="npcStatus"
        class="de.mbws.common.data.generated.NpcStatus"
        outer-join="auto"
    />
    <!-- bi-directional one-to-many association to NpcWorldobjectMapping -->

    <set
        name="npcWorldobjectMappings"
        lazy="true"
        inverse="true"
		cascade="all"

    >
        <key>
            <column name="NPC_ID" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.NpcWorldobjectMapping"
        />
    </set>
    <!-- bi-directional one-to-many association to NpcSkillMapping -->

    <set
        name="npcSkillMappings"
        lazy="true"
        inverse="true"
		cascade="all"

    >
        <key>
            <column name="NPC_ID" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.NpcSkillMapping"
        />
    </set>

</class>
</hibernate-mapping>
