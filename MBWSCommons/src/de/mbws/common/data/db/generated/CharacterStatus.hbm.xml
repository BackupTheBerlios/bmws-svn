<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.db.generated.CharacterStatus" 
    table="character_status"
    lazy="false"
>

    <id
        name="id"
        type="java.lang.Long"
        column="id"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="charstatus"
        type="java.lang.String"
        column="charstatus"
        not-null="true"
        length="1"
    />
    <property
        name="pvp"
        type="java.lang.String"
        column="pvp"
        not-null="true"
        length="1"
    />
    <property
        name="gamestatus"
        type="java.lang.String"
        column="gamestatus"
        not-null="true"
        length="1"
    />
    <property
        name="coordinateX"
        type="int"
        column="coordinate_x"
        not-null="true"
        length="10"
    />
    <property
        name="coordinateY"
        type="int"
        column="coordinate_y"
        not-null="true"
        length="10"
    />
    <property
        name="coordinateZ"
        type="int"
        column="coordinate_z"
        not-null="true"
        length="10"
    />
    <property
        name="currentmana"
        type="int"
        column="currentmana"
        not-null="true"
        length="10"
    />
    <property
        name="currenthealth"
        type="int"
        column="currenthealth"
        not-null="true"
        length="10"
    />
    <property
        name="currentstamina"
        type="int"
        column="currentstamina"
        not-null="true"
        length="10"
    />
    <property
        name="currentstrength"
        type="int"
        column="currentstrength"
        not-null="true"
        length="10"
    />
    <property
        name="currentinteligence"
        type="int"
        column="currentinteligence"
        not-null="true"
        length="10"
    />
    <property
        name="currentdexterity"
        type="int"
        column="currentdexterity"
        not-null="true"
        length="10"
    />
    <property
        name="currentconstitution"
        type="int"
        column="currentconstitution"
        not-null="true"
        length="10"
    />
    <property
        name="freexp"
        type="int"
        column="freexp"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to Characterdata -->
    <one-to-one
        name="characterdata"
        class="de.mbws.common.data.db.generated.Characterdata"
        outer-join="auto"
        constrained="true"
    />
    <!-- bi-directional many-to-one association to Map -->
    <many-to-one
        name="map"
        class="de.mbws.common.data.db.generated.Map"
        not-null="true"
    >
        <column name="map_id" />
    </many-to-one>

</class>
</hibernate-mapping>
