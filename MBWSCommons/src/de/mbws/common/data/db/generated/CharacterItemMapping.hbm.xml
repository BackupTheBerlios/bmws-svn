<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.db.generated.CharacterItemMapping" 
    table="character_item_mapping"
    lazy="false"
>

    <id
        name="id"
        type="java.lang.Long"
        column="id"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="position"
        type="java.lang.String"
        column="position"
        not-null="true"
        length="1"
    />
    <property
        name="currentcondition"
        type="int"
        column="currentcondition"
        not-null="true"
        length="10"
    />
    <property
        name="depositItem"
        type="int"
        column="deposit_item"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Item -->
    <many-to-one
        name="item"
        class="de.mbws.common.data.db.generated.Item"
        not-null="true"
    >
        <column name="item_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Characterdata -->
    <many-to-one
        name="characterdata"
        class="de.mbws.common.data.db.generated.Characterdata"
        not-null="true"
    >
        <column name="characterdata_id" />
    </many-to-one>

</class>
</hibernate-mapping>
