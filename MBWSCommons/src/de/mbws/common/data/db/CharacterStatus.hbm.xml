<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.generated.CharacterStatus" 
    table="character_status"
    lazy="false"
>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="charstatus"
        type="java.lang.String"
        column="CHARSTATUS"
        not-null="true"
        length="1"
    />
    <property
        name="pvp"
        type="java.lang.String"
        column="PVP"
        not-null="true"
        length="1"
    />
    <property
        name="gamestatus"
        type="java.lang.String"
        column="GAMESTATUS"
        not-null="true"
        length="1"
    />
    <property
        name="coordinateX"
        type="int"
        column="COORDINATE_X"
        not-null="true"
        length="10"
    />
    <property
        name="coordinateY"
        type="int"
        column="COORDINATE_Y"
        not-null="true"
        length="10"
    />
    <property
        name="coordinateZ"
        type="int"
        column="COORDINATE_Z"
        not-null="true"
        length="10"
    />
    <property
        name="currentmana"
        type="int"
        column="CURRENTMANA"
        not-null="true"
        length="10"
    />
    <property
        name="currenthealth"
        type="int"
        column="CURRENTHEALTH"
        not-null="true"
        length="10"
    />
    <property
        name="currentstamina"
        type="int"
        column="CURRENTSTAMINA"
        not-null="true"
        length="10"
    />
    <property
        name="currentstrength"
        type="int"
        column="CURRENTSTRENGTH"
        not-null="true"
        length="10"
    />
    <property
        name="currentinteligence"
        type="int"
        column="CURRENTINTELIGENCE"
        not-null="true"
        length="10"
    />
    <property
        name="currentdexterity"
        type="int"
        column="CURRENTDEXTERITY"
        not-null="true"
        length="10"
    />
    <property
        name="currentconstitution"
        type="int"
        column="CURRENTCONSTITUTION"
        not-null="true"
        length="10"
    />
    <property
        name="freexp"
        type="int"
        column="FREEXP"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-one association to Characterdata -->
    <one-to-one
        name="characterdata"
        class="de.mbws.common.data.generated.Characterdata"
        outer-join="auto"
        constrained="true"
    />
    <!-- bi-directional many-to-one association to Map -->
    <many-to-one
        name="map"
        class="de.mbws.common.data.generated.Map"
        not-null="true"
    >
        <column name="MAP_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
