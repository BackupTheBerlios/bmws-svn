<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="de.mbws.common.data.generated.Worldobject" 
    table="worldobject"
    lazy="false"
>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="NAME"
        not-null="true"
        length="255"
    />
    <property
        name="description"
        type="java.lang.String"
        column="DESCRIPTION"
        not-null="true"
        length="65535"
    />
    <property
        name="coordinateX"
        type="int"
        column="COORDINATE_X"
        not-null="true"
        length="10"
    />
    <property
        name="coordinateY"
        type="int"
        column="COORDINATE_Y"
        not-null="true"
        length="10"
    />
    <property
        name="coordinateZ"
        type="int"
        column="COORDINATE_Z"
        not-null="true"
        length="10"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to NpcWorldobjectMapping -->

    <set
        name="npcWorldobjectMappings"
        lazy="true"
        inverse="true"
		cascade="all"

    >
        <key>
            <column name="WORLDOBJECT_ID" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.NpcWorldobjectMapping"
        />
    </set>
    <!-- bi-directional one-to-many association to CharacterWorldobjectMapping -->

    <set
        name="characterWorldobjectMappings"
        lazy="true"
        inverse="true"
		cascade="all"

    >
        <key>
            <column name="WORLDOBJECT_ID" />
        </key>
        
        <one-to-many 
            class="de.mbws.common.data.generated.CharacterWorldobjectMapping"
        />
    </set>
    <!-- bi-directional many-to-one association to WorldobjectType -->
    <many-to-one
        name="worldobjectType"
        class="de.mbws.common.data.generated.WorldobjectType"
        not-null="true"
    >
        <column name="WORLDOBJECT_TYPE_ID" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Map -->
    <many-to-one
        name="map"
        class="de.mbws.common.data.generated.Map"
        not-null="true"
    >
        <column name="MAP_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
